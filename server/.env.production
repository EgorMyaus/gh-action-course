# Production Environment
# Uses PostgreSQL RDS database

PORT=3001
NODE_ENV=production
USE_DATABASE=true

# These values come from AWS Secrets Manager in production
# For local testing with production config, fill these in:
DB_HOST=your-rds-endpoint.amazonaws.com
DB_PORT=5432
DB_NAME=reactapp
DB_USER=dbadmin
DB_PASSWORD=from_secrets_manager
DB_SSL=true
